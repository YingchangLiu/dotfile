lock '~/.config/bin/wayland_session_lock'
timeout 300  '~/.config/bin/screenoff'
timeout 1801 '~/.config/bin/wayland_session_lock' 
timeout 1831 'hyprctl dispatch dpms off eDP-1'
timeout 2701 'systemctl suspend' 
before-sleep '~/.config/bin/wayland_session_lock' 
unlock 'hyprctl dispatch dpms on eDP-1'
after-resume 'hyprctl dispatch dpms on eDP-1'

